{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "lock": false,
  "tasks": {
    "cache": "deno cache main.ts",
    "check": "deno check main.ts",
    "compile": "deno compile -A main.ts",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "fmt": "deno run --allow-read --allow-write --allow-sys --allow-env npm:prettier . --write",
    "lint": "deno lint",
    "start": "deno run -A main.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.4.2/",
    "@components/": "./components/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.3",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.2.3",
    "@typings/": "./typings/",
    "harmony/": "https://deno.land/x/harmony@v2.8.0/",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.1",
    "preact/": "https://esm.sh/preact@10.15.1/",
    "preact": "https://esm.sh/preact@10.15.1",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    },
    "exclude": [
      "_fresh"
    ]
  },
  "fmt": {
    "exclude": [
      "_fresh"
    ]
  }
}